# Display control

record(mbbo, "$(P)$(R)DCB_Ch") {
    field(DESC, "Display control bar graph")
    field(ZRST, "Channel 1")
    field(ONST, "Channel 2")
    field(TWST, "Channel 3")
}

record(mbbi, "$(P)$(R)DCB_Ch_RBV") {
    field(DESC, "Display control bar graph")
    field(ZRST, "Channel 1")
    field(ONST, "Channel 2")
    field(TWST, "Channel 3")
}

record(mbbo, "$(P)$(R)DCB") {
    field(DESC, "Display control bar graph")
    field(DTYP, "stream")
    field(ZRST, "Off")
    field(ONST, "Bar: FR")
    field(TWST, "Bar: FR HL")
    field(THST, "Bar: FR setpoint")
    field(FRST, "Bar: DEC")
    field(FVST, "Bar: DEC HL")
    field(SXST, "Bar: DEC setpoint")
    field(SVST, "Graph: 0.2 s/pixel")
    field(EIST, "Graph: 1 s/pixel")
    field(NIST, "Graph: 6 s/pixel")
    field(TEST, "Graph: 1 min/pixel")
    field(OUT, "@devVGC501.proto DCB($(P)$(R)) $(PORT) $(A)")
}

record(mbbi, "$(P)$(R)DCB_RBV") {
    field(DESC, "Display control bar graph RBV")
    field(DTYP, "stream")
    field(ZRST, "Off")
    field(ONST, "Bar: FR")
    field(TWST, "Bar: FR HL")
    field(THST, "Bar: FR setpoint")
    field(FRST, "Bar: DEC")
    field(FVST, "Bar: DEC HL")
    field(SXST, "Bar: DEC setpoint")
    field(SVST, "Graph: 0.2 s/pixel")
    field(EIST, "Graph: 1 s/pixel")
    field(NIST, "Graph: 6 s/pixel")
    field(TEST, "Graph: 1 min/pixel")
    field(INP, "@devVGC501.proto getDCB($(P)$(R)) $(PORT) $(A)")
    field(SCAN, "5 second")
}

